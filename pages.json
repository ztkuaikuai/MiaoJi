			}
		]
	},
	"uniIdRouter": {
		"loginPage": "uni_modules/uni-id-pages/pages/login/login-withoutpwd",
		"needLogin": [
			"pagesMy/.*",
			"pagesAccount/.*",
			"pagesFilter/.*"
		],
		"resToLogin": true
